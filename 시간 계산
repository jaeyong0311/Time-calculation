#include <stdio.h>
#include <math.h>
#define PI 3.141592
int main()
{
    int h, m, s, x, y;
    double deg;
    scanf("%d:%d:%d", &h, &m, &s);

    deg=(double)h*360/12*(PI/180);
    x=120*sin(deg);
    y=120*cos(deg);
    printf("(%d, %d)\n", x, y);

    deg=(double)m*360/60*(PI/180);
    x=200*sin(deg);
    y=200*cos(deg);
    printf("(%d, %d)\n", x, y);

    deg=(double)s*360/60*(PI/180);
    x=180*sin(deg);
    y=180*cos(deg);
    printf("(%d, %d)\n", x, y);
    
    return 0;
}
